{
    "name": "gocobachi/compressy",
    "type": "library",
    "description": "Compressy, the archive manager companion a child of the Alchemy/Zippy",
    "keywords": ["zip", "tar", "bzip", "compression"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alchemy",
            "email": "dev.team@alchemy.fr",
            "homepage": "http://www.alchemy.fr/"
        },
        {
            "name": "Miguel Gocobachi",
            "email": "mgocobachi@php.net"
        }
    ],
    "require": {
        "php": ">=7.3",
        "doctrine/collections": "^1.0",
        "symfony/filesystem": "^4.0 || ^5.0",
        "symfony/process": "^4.0 || ^5.0",
        "symfony/polyfill-mbstring": "^1.3"
    },
    "require-dev": {
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^7.0",
        "phpunit/phpunit": "^8.0",
        "symfony/finder": "^4.0 || ^5.0"
    },
    "suggest": {
        "ext-zip": "To use the ZipExtensionAdapter",
        "guzzlehttp/guzzle": "To use the GuzzleTeleporter with Guzzle"
    },
    "autoload": {
        "psr-4": {
            "Gocobachi\\Compressy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gocobachi\\Compressy\\Functional\\": "tests/Functional/",
            "Gocobachi\\Compressy\\Tests\\": "tests/Tests/"
        }
    }
}